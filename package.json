{
  "name": "intervalai-server",
  "version": "1.0.0",
  "description": "IntervalAI - Neural-enhanced spaced repetition learning system backend",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "mocha": "mocha --exit --file test/setup.test.js '!(node_modules)/**/*test.js'",
    "mocha-exit0": "mocha --exit --file test/setup.test.js '!(node_modules)/**/*test.js' || true",
    "test": "nodemon --exec npm run mocha-exit0",
    "cover": "nodemon --exec nyc --reporter=lcov --reporter=text-summary npm run mocha-exit0",
    "build": "node index.js",
    "travis": "npm run mocha"
  },
  "author": "IntervalAI Team",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs": "^4.23.0-rc.0",
    "@tensorflow/tfjs-node-gpu": "^4.22.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "create-react-app": "^2.0.4",
    "dotenv": "latest",
    "eslint": "^5.7.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.3",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0"
  }
}
